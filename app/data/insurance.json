{
  "insurance_types": {
    "health": {
      "title": "Health Insurance",
      "icon": "üè•",
      "description": "International & local health coverage",
      "required_fields": [
        { "name": "age", "label": "Age", "type": "number", "min": 0, "max": 100 },
        { "name": "country", "label": "Country of residence", "type": "select", "options": ["Thailand", "UAE", "Qatar", "Vietnam", "Saudi Arabia"] },
        { "name": "coverage", "label": "Coverage limit", "type": "select", "options": ["50,000 USD", "100,000 USD", "1M USD", "No Limit"] }
      ],
      "optional_fields": [
        { "name": "pre_existing_conditions", "label": "Pre-existing conditions", "type": "boolean" },
        { "name": "dental", "label": "Dental coverage", "type": "boolean" },
        { "name": "maternity", "label": "Maternity coverage", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 250,
        "factors": {
          "age": { "18-30": 1.1, "31-45": 1.4, "46-60": 2.0, "60+": 2.8 },
          "coverage": { "50,000 USD": 1, "100,000 USD": 1.2, "1M USD": 1.6, "No Limit": 2.5 },
          "pre_existing_conditions": 1.4,
          "dental": 1.15,
          "maternity": 1.3
        }
      }
    },
    "car": {
      "title": "Car Insurance",
      "icon": "üöó",
      "description": "Full coverage & third-party options",
      "required_fields": [
        { "name": "car_make", "label": "Car Brand", "type": "text" },
        { "name": "car_model", "label": "Car Model", "type": "text" },
        { "name": "year", "label": "Year", "type": "select" },
        { "name": "mileage", "label": "Annual Mileage (km)", "type": "select", "options": ["0-5,000", "5,000-10,000", "10,000-15,000", "15,000-20,000", "20,000-30,000", "30,000+"] },
        { "name": "car_value", "label": "Car Value (USD)", "type": "number", "min": 1000, "max": 500000 },
        { "name": "usage", "label": "Vehicle Usage", "type": "select", "options": ["Personal", "Business", "Commute", "Taxi/Ride-sharing", "Delivery"] },
        { "name": "driver_age", "label": "Driver Age", "type": "number", "min": 18, "max": 99 },
        { "name": "marital_status", "label": "Marital Status", "type": "select", "options": ["Single", "Married", "Divorced", "Widowed"] },
        { "name": "gender", "label": "Gender", "type": "select", "options": ["Male", "Female", "Other"] },
        { "name": "driving_experience", "label": "Years of Driving Experience", "type": "number", "min": 0, "max": 80 }
      ],
      "coverage_options": [
        { "name": "third_party", "label": "Third-Party Liability", "base_multiplier": 1, "description": "Covers damage to others" },
        { "name": "comprehensive", "label": "Comprehensive Coverage", "base_multiplier": 1.8, "description": "Full protection for your vehicle" },
        { "name": "collision", "label": "Collision Coverage", "base_multiplier": 1.4, "description": "Covers accident damage" },
        { "name": "theft_protection", "label": "Theft & Vandalism", "base_multiplier": 1.3, "description": "Protection against theft" }
      ],
      "optional_fields": [
        { "name": "no_claim_bonus", "label": "No-claim bonus (discount)", "type": "boolean", "description": "Have you been claim-free for 1+ years?" },
        { "name": "additional_drivers", "label": "Additional drivers", "type": "boolean", "description": "Add family members as drivers" },
        { "name": "roadside_assistance", "label": "24/7 Roadside Assistance", "type": "boolean", "description": "Towing, flat tire, battery jump" },
        { "name": "replacement_car", "label": "Replacement Car Service", "type": "boolean", "description": "Free car while yours is repaired" },
        { "name": "windscreen_coverage", "label": "Windscreen & Glass Coverage", "type": "boolean", "description": "Repair/replace windscreen" },
        { "name": "natural_disaster", "label": "Natural Disaster Coverage", "type": "boolean", "description": "Flood, earthquake, storm damage" },
        { "name": "personal_accident", "label": "Personal Accident Insurance", "type": "boolean", "description": "Medical expenses for driver/passengers" }
      ],
      "calculation_formula": {
        "base": 180,
        "factors": {
          "year": { "old": 1.3, "normal": 1, "new": 0.85 },
          "mileage": { 
            "0-5,000": 0.9, 
            "5,000-10,000": 1, 
            "10,000-15,000": 1.1, 
            "15,000-20,000": 1.2, 
            "20,000-30,000": 1.3, 
            "30,000+": 1.5 
          },
          "car_value": { "per_10k": 0.05 },
          "usage": { 
            "Personal": 1, 
            "Business": 1.2, 
            "Commute": 1.15, 
            "Taxi/Ride-sharing": 1.8, 
            "Delivery": 1.6 
          },
          "driver_age": { 
            "18-24": 1.5, 
            "25-30": 1.2, 
            "31-50": 1, 
            "51-65": 1.1, 
            "65+": 1.3 
          },
          "marital_status": { 
            "Single": 1.1, 
            "Married": 0.95, 
            "Divorced": 1.05, 
            "Widowed": 1 
          },
          "gender": { 
            "Male": 1.1, 
            "Female": 1, 
            "Other": 1 
          },
          "driving_experience": { 
            "0-2": 1.4, 
            "3-5": 1.2, 
            "6-10": 1, 
            "11+": 0.9 
          },
          "no_claim_bonus": 0.85,
          "additional_drivers": 1.15,
          "roadside_assistance": 1.08,
          "replacement_car": 1.12,
          "windscreen_coverage": 1.05,
          "natural_disaster": 1.2,
          "personal_accident": 1.1
        }
      }
    },
    "business": {
      "title": "Business / Commercial Insurance",
      "icon": "üè¢",
      "description": "Shops, restaurants, offices, warehouses",
      "required_fields": [
        { "name": "business_type", "label": "Business Type", "type": "select", "options": ["Restaurant", "Retail", "Office", "Warehouse", "E-commerce"] },
        { "name": "annual_revenue", "label": "Annual Revenue (USD)", "type": "number", "min": 0 },
        { "name": "employees_count", "label": "Number of employees", "type": "number", "min": 1 }
      ],
      "optional_fields": [
        { "name": "public_liability", "label": "Public Liability", "type": "boolean" },
        { "name": "equipment_coverage", "label": "Equipment Coverage", "type": "boolean" },
        { "name": "cyber_insurance", "label": "Cyber Insurance", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 400,
        "factors": {
          "business_type": {
            "Restaurant": 1.6,
            "Retail": 1.3,
            "Office": 1,
            "Warehouse": 1.8,
            "E-commerce": 1.1
          },
          "employees_count": { "per_employee": 0.02 },
          "public_liability": 1.25,
          "equipment_coverage": 1.35,
          "cyber_insurance": 1.2
        }
      }
    },
    "home": {
      "title": "Home / Condo Insurance",
      "icon": "üè°",
      "description": "Protect your condo, villa or apartment",
      "required_fields": [
        { "name": "property_value", "label": "Property Value (USD)", "type": "number", "min": 10000 },
        { "name": "location", "label": "Location", "type": "select", "options": ["Bangkok", "Pattaya", "Dubai", "Riyadh", "Doha", "Hanoi", "Ho Chi Minh"] }
      ],
      "optional_fields": [
        { "name": "flood_protection", "label": "Flood Protection", "type": "boolean" },
        { "name": "earthquake", "label": "Earthquake Coverage", "type": "boolean" },
        { "name": "contents", "label": "Contents Insurance", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 120,
        "factors": {
          "property_value": { "per_100k": 0.15 },
          "location": { "Bangkok": 1.1, "Pattaya": 1, "Dubai": 1.3, "Riyadh": 1.4, "Doha": 1.25, "Hanoi": 1, "Ho Chi Minh": 1.05 },
          "flood_protection": 1.2,
          "earthquake": 1.3,
          "contents": 1.15
        }
      }
    },
    "travel": {
      "title": "Travel Insurance",
      "icon": "üåç",
      "description": "Medical, trip cancellation and worldwide assistance",
      "required_fields": [
        { "name": "destination", "label": "Destination", "type": "text", "placeholder": "e.g. Europe, USA, Asia" },
        { "name": "duration_days", "label": "Duration (days)", "type": "number", "min": 1, "max": 365 },
        { "name": "coverage", "label": "Coverage", "type": "select", "options": ["50,000 USD", "100,000 USD", "500,000 USD"] }
      ],
      "optional_fields": [
        { "name": "adventure_sports", "label": "Adventure Sports", "type": "boolean" },
        { "name": "trip_cancellation", "label": "Trip Cancellation", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 2,
        "factors": {
          "duration_days": { "per_day": 1 },
          "coverage": { "50,000 USD": 1, "100,000 USD": 1.2, "500,000 USD": 1.7 },
          "adventure_sports": 1.5,
          "trip_cancellation": 1.25
        }
      }
    },
    "courier": {
      "title": "Courier / Delivery Insurance",
      "icon": "üì¶",
      "description": "Protect your deliveries and shipments",
      "required_fields": [
        { "name": "shipment_value", "label": "Average Shipment Value (USD)", "type": "number", "min": 0 },
        { "name": "monthly_shipments", "label": "Monthly Shipments", "type": "number", "min": 1 },
        { "name": "shipment_type", "label": "Shipment Type", "type": "select", "options": ["Documents", "Electronics", "Food", "General Goods", "Fragile Items"] }
      ],
      "optional_fields": [
        { "name": "international", "label": "International Coverage", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 50,
        "factors": {
          "shipment_type": {
            "Documents": 0.8,
            "Electronics": 1.5,
            "Food": 1.2,
            "General Goods": 1,
            "Fragile Items": 1.7
          },
          "monthly_shipments": { "per_10": 0.05 },
          "international": 1.4
        }
      }
    },
    "liability": {
      "title": "Professional Liability (RC Pro)",
      "icon": "üßæ",
      "description": "Professional indemnity insurance",
      "required_fields": [
        { "name": "profession", "label": "Profession", "type": "select", "options": ["Consultant", "Doctor", "Lawyer", "Architect", "Engineer", "Accountant", "IT Professional"] },
        { "name": "annual_revenue", "label": "Annual Revenue (USD)", "type": "number", "min": 0 },
        { "name": "coverage_limit", "label": "Coverage Limit", "type": "select", "options": ["100,000 USD", "500,000 USD", "1M USD", "5M USD"] }
      ],
      "optional_fields": [
        { "name": "cyber_liability", "label": "Cyber Liability", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 300,
        "factors": {
          "profession": {
            "Consultant": 1,
            "Doctor": 2.5,
            "Lawyer": 2,
            "Architect": 1.8,
            "Engineer": 1.6,
            "Accountant": 1.4,
            "IT Professional": 1.3
          },
          "coverage_limit": { "100,000 USD": 1, "500,000 USD": 1.5, "1M USD": 2, "5M USD": 3 },
          "cyber_liability": 1.3
        }
      }
    },
    "retail": {
      "title": "Shop / Retail Insurance",
      "icon": "üè¨",
      "description": "Complete coverage for retail businesses",
      "required_fields": [
        { "name": "shop_size_sqm", "label": "Shop Size (sqm)", "type": "number", "min": 10 },
        { "name": "inventory_value", "label": "Inventory Value (USD)", "type": "number", "min": 0 },
        { "name": "location_type", "label": "Location", "type": "select", "options": ["Mall", "Street Shop", "Shopping Center", "Standalone"] }
      ],
      "optional_fields": [
        { "name": "theft_protection", "label": "Enhanced Theft Protection", "type": "boolean" },
        { "name": "glass_coverage", "label": "Glass Coverage", "type": "boolean" }
      ],
      "calculation_formula": {
        "base": 200,
        "factors": {
          "shop_size_sqm": { "per_10sqm": 0.08 },
          "location_type": {
            "Mall": 0.9,
            "Street Shop": 1.2,
            "Shopping Center": 1,
            "Standalone": 1.3
          },
          "theft_protection": 1.2,
          "glass_coverage": 1.1
        }
      }
    }
  }
}
